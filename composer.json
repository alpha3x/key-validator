{
    "name": "alpha3x/key-validator",
    "description": "This package will validate API keys & secrets, Tokens, HMAC.",
    "keywords": [
        "alpha3x",
        "key-validator"
    ],
    "homepage": "https://github.com/alpha3x/key-validator",
    "license": "MIT",
    "authors": [
        {
            "name": "Shuvro Roy",
            "email": "shuvro.nsu.cse@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Akram Hossain",
            "email": "ak.khan.kp@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "pestphp/pest": "^3.4",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Alpha3x\\KeyValidator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alpha3x\\KeyValidator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
